{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Pedido",
    "type": "object",
    "required": ["cliente", "itens", "mesa"],
    "properties": {
      "cliente": { "type": "string" },
      "itens": {
        "type": "array",
        "minItems": 1,
        "items": { "type": "string" }
      },
      "mesa": { "type": "integer", "minimum": 1 },
      "observacoes": { "type": "string" }
    }
  }
  